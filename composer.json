{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:elsuterino/laravel-nova.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "barryvdh/laravel-snappy": "^0.4.6",
        "dillingham/nova-items-field": "^0.5.0",
        "ebess/advanced-nova-media-library": "^2.9",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "inertiajs/inertia-laravel": "^0.1.3",
        "inspheric/nova-url-field": "^1.32",
        "kabbouchi/nova-logs-tool": "^0.1.3",
        "laravel-frontend-presets/bulma": "^3.0",
        "laravel/framework": "^6.0",
        "laravel/nova": "^2.1@dev",
        "laravel/tinker": "^1.0",
        "laravel/ui": "^1.0",
        "naxon/nova-field-sortable": "^1.0",
        "nikaia/nova-rating-field": "^1.0",
        "spatie/eloquent-sortable": "^3.7",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/regex": "^1.4",
        "symfony/process": "^4.3",
        "timothyasp/nova-color-field": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Elsuterino\\": "app/Elsuterino"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
